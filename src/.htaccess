# SEO friendly url's.
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L]

ErrorDocument 401 https://lenardsilvester.nl/
ErrorDocument 403 https://lenardsilvester.nl/
ErrorDocument 404 https://lenardsilvester.nl/
ErrorDocument 500 https://lenardsilvester.nl/
ErrorDocument 502 https://lenardsilvester.nl/
ErrorDocument 503 https://lenardsilvester.nl/
ErrorDocument 503 https://lenardsilvester.nl/
